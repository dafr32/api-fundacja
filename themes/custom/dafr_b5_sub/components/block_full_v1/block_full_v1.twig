<div {{ attributes }}>  
  <article class="block_full_v1 block_full">
    <div class="row ">
      <div class="col-12 col-lg-5 left">        
          <div class="block-image">
            <div class="shape">
                <div class="image">
                  {% block image %}{% endblock %}
                </div>
            </div>
          </div>        
      </div>
      <div class="col-12 col-lg-7 right">
        <div class="block-content">
            {% block content %}{% endblock %}          
        </div>
      </div>
    </div>
  </article>
</div>
